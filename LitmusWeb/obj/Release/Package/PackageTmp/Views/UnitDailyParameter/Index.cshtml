@model LitmusWeb.Models.MasterUnitModelForUnitAdmin
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">
                            Registered Units
                        </h4>
                        <p class="card-description">
                            Unit Details <small>(Details of all Units)</small>
                        </p>
                        <div class="table-responsive pt-3">
                            <table class="table table-bordered table-striped">
                                <thead class="table-dark">
                                    <tr>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.Code)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.CrushingSeason)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.CrushingStartDate)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.CrushingEndDate)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.DayHours)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.EntryDate)
                                        </th>
                                        <th nowrap>
                                            @Html.DisplayNameFor(model => model.ProcessDate)
                                        </th>
                                        <th nowrap>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.Code)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.CrushingSeason)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.CrushingStartDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.CrushingEndDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.DayHours)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.EntryDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => Model.ProcessDate)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { } , new { @class = "btn btn-xs btn-info" })
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




