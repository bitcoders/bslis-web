    <script>
       $(document).ready(function () {
           var selected_unit;
           
           selected_unit = @Session["BaseUnitCode"];           
           var param = { 'unit_code': selected_unit }; 
           
    $.ajax({
        type: "GET",
        url: "/api/TwoHourlySummary/TwoHourlySummaryByHours",
        data: { 'unitCode': selected_unit },
        contentType: "application/json",
        dataType: 'json',
        success: (data, txtStatus, jqXHR) => {
            if (jqXHR.status == 200) {
                var row = '<tbody><tr class="text-nowrap">';
                for (var i = 0; i <= 2; i++) {
                    $.each(data[i], function (index, value) {
                        row += '<td>' + value + '</td>';
                    });
                    row += '</tr>';
                    i = i++;
                }
                row += '</tbody>';
                $("#sortable-table-2").append(row);
            } else {
                console.log(jqXHR);
            }
        }, Error: (error) => {
            console.error(error);
        }
    });
           
           $.ajax({
               type: 'POST',
               url: '/api/TwoHourlySummary',
               data: JSON.stringify(param),
               contentType: 'application/json; charset=utf-8',
               dataType: 'json',
               success: (data, txtStatus, jqXHR) => {
                   if (jqXHR.status == 200) {
                       var row = '<tbody><tr class="text-nowrap">';
                       for (var i = 0; i <= 2; i++) {
                           $.each(data[i], function (index, value) {
                               row += '<td>' + value + '</td>';
                           });
                           row += '</tr>';
                           i = i++;
                       }
                       row += '</tbody>';
                       $("#sortable-table-3").append(row);
                   }
                   else {
                       console.log(jqXHR);
                   }
               }
               , Error: (error) => {
                   console.error(error);
               }
           });
});
    </script>
