@model LitmusWeb.Models.DailyAnalysisModel
@{
    ViewBag.Title = Session["UnitName"].ToString();
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">

            <div class="col-12 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">
                            Daily Analyses
                            <span class="card-description">
                                Entry for - @ViewBag.EntryDate
                            </span>
                        </h4>

                    </div>
                    <div class="card-body dashboard-tabs p-0">
                        <ul class="nav nav-tabs px-4 border-left-0 border-top-0 border-right-0" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="overview-tab" data-toggle="tab" href="#page_one" role="tab" aria-controls="overview" aria-selected="true">Tab-1</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="icumsa-tab" data-toggle="tab" href="#icumsa" role="tab" aria-controls="icumsa" aria-selected="false">Tab-2</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab3-tab" data-toggle="tab" href="#Tab3" role="tab" aria-controls="Tab3" aria-selected="false">Tab-3</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab4-tab" data-toggle="tab" href="#Tab4" role="tab" aria-controls="Tab4" aria-selected="false">Tab-4</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab5-tab" data-toggle="tab" href="#Tab5" role="tab" aria-controls="Tab5" aria-selected="false">Tab-5</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab6-tab" data-toggle="tab" href="#Tab6" role="tab" aria-controls="Tab6" aria-selected="false">Tab-6</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab7-tab" data-toggle="tab" href="#Tab7" role="tab" aria-controls="Tab7" aria-selected="false">Tab-7</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab8-tab" data-toggle="tab" href="#Tab8" role="tab" aria-controls="Tab8" aria-selected="false">Tab-8</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="Tab9-tab" data-toggle="tab" href="#Tab9" role="tab" aria-controls="Tab9" aria-selected="false">Tab-9(Distillery)</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link count-indicator" id="Tab10-tab" data-toggle="tab" href="#Tab10" role="tab" aria-controls="Tab10" aria-selected="false">
                                    Tab-10(Other Data)
                                    <span class="badge badge-primary">New</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="finish-tab" data-toggle="tab" href="#Finish" role="tab" aria-controls="finish" aria-selected="false">Finish & Save</a>
                            </li>
                        </ul>

                        @if (ViewBag.InitialState == "New")
                        {
                            TempData["ActionName"] = "Index";
                        }
                        else
                        {
                            TempData["ActionName"] = "Edit";
                        }
                        @using (Html.BeginForm(TempData["ActionName"].ToString(), "DailyAnalysis", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("InitialState", TempData["InitialState"])
                            @Html.Hidden("hiddenUnitCode", Session["BaseUnitCode"])
                            <div class="tab-content py-0 px-0 border-left-0 border-bottom-0 border-right-0">

                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabOne.cshtml")


                                <!-- Tab-2 Start-->
                                @{
                                    Html.RenderPartial("~/Views/DailyAnalysis/_PartialTabTwo.cshtml");
                                }
                                <!-- Tab-2 End-->
                                <!-- tab-3 start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabThree.cshtml")

                                <!-- tab-3 end -->
                                <!--Tab-4 Start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabFour.cshtml")
                                <!-- Tab-4 End-->
                                <!--Tab-5 Start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabFive.cshtml")
                                <!--Tab-5 End-->
                                <!-- Tab-6 Start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabSix.cshtml")

                                <!-- Tab-6 End-->
                                <!--Tab-7 Start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabSeven.cshtml")
                                <!--Tab 7 End-->
                                <!--Tab8 Store Consumption-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabEight.cshtml")
                                <!--Tab 8 End-->
                                <!--Tab9 Distillery-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabNine.cshtml")
                                <!--/Tab 9 Distillery End-->
                                <!-- Tab-10 Other data like Wages and Store Consumables-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabTen.cshtml")

                                <!-- End of tab 10-->
                                <!--Tab-11 finish Start-->
                                @Html.Partial("~/Views/DailyAnalysis/_PartialTabEleven.cshtml")
                                <!--Tab 11 Finish End-->
                            </div><!--/ end of tab content-->
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    <link href="~/Content/MainContent/css/floating-labels.css" rel="stylesheet" />
    <script src="~/Scripts/js/dashboard.js"></script>
    <script src="~/Scripts/SugarAnalysisJs/DayAnalysis-2.2.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

